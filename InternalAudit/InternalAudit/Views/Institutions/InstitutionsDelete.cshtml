@model InternalAudit.Models.TblMasterInstitutions

@{
    ViewData["Title"] = "InstitutionsDelete";
    Layout = "~/Views/Shared/Internal.cshtml";
}
<div class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">
                <div class="card">
                    <div class="card-header card-header-primary">
                        <div class="row">
                            <div class="col-md-5">
                                <h4 class="card-title">Institutions Master</h4>
                            </div>
                            <div class="col-md-7">
                                @Html.ActionLink("Back", "InstitutionsDetails", "Institutions", null, new { @class = "btn btn-default pull-right" })
                            </div>
                        </div>
                    </div><br />

                    <form asp-action="Delete" method="post">
                        <div class="form-group">
                            <h6 class="p-4">Are you sure you want to delete this</h6>
                            <input type="hidden" asp-for="InstitutionsRecNo" />
                        </div>
                        <div class="card-footer text-center">
                            <div class="row col-12" style="justify-content:center;">
                                <div class="col-1">
                                    <input type="submit" value="Delete" class="btn btn-primary" />
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
@if (ViewData["Msg"] != null)
{
    InternalAudit.DTO.MessageDTO msg = ViewData["Msg"] as InternalAudit.DTO.MessageDTO;
    @await Html.PartialAsync("~/Views/Shared/_ShowMsg.cshtml", msg);
}















