@model InternalAudit.Models.TblMasterFinancialyear
@{
    ViewBag.Title = "Financial_Year";
    Layout = "~/Views/Shared/Internal.cshtml";
}
    <script src="~/js/jquery.min.js"></script>
<div class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">
                <div class="card">
                    <div class="card-header card-header-primary">
                        <h4 class="card-title">Financial Year</h4>
                        <p class="card-category">Add Financial Year </p>
                        @Html.ActionLink("Back to List", "Financial_Year_List", "Financial_Year", null, new { @class = "btn btn-default pull-right", style = "margin-top: -49px;" })
                    </div>
                    <div class="card-body">
                        <form asp-action="Financial_Year">
                            <div class="ShadowBox">
                                <div class="form1">
                                    <div class="form-group ">
                                        <label for="email">Financial Year</label>
                                        <input asp-for="FinancialYear" class="form-control" id="name">
                                    </div>

                                </div>
                                <div class="col-lg-offset-4">
                                    <input type="submit" value="Save" class="btn btn-outline-primary button" name="btnSubmit" />
                                </div><br />
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
    @if (ViewData["Msg"] != null)
    {
        InternalAudit.DTO.MessageDTO msg = ViewData["Msg"] as InternalAudit.DTO.MessageDTO;
        @await Html.PartialAsync("~/Views/Shared/_ShowMsg.cshtml", msg);
    }
</div>

