@model InternalAudit.Models.TblMasterFinancialyear

@{
    ViewData["Title"] = "EditFinancial_Year";
    Layout = "~/Views/Shared/Internal.cshtml";
}
<script src="~/js/jquery.min.js"></script>
@if (ViewData["Msg"] != null)
{
    InternalAudit.DTO.MessageDTO msg = ViewData["Msg"] as InternalAudit.DTO.MessageDTO;
    @await Html.PartialAsync("~/Views/Shared/_ShowMsg.cshtml", msg);
}
<div class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">
                <div class="card">
                    <div class="card-header card-header-primary">
                        <h4 class="card-title">Financial Year</h4>
                        <p class="card-category">Financial Year List </p>
                        @Html.ActionLink("Back to List", "Financial_Year_List", "Financial_Year", null, new { @class = "btn btn-default pull-right", style = "margin-top: -49px;" })
                    </div>
                    <div class="card-body">
                        <form asp-action="EditFinancial_Year">
                            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                            <input type="hidden" asp-for="FinancialYearRecNo" />
                            <div class="form-group">
                                <label asp-for="FinancialYear" class="control-label">Financial Year</label>
                                <input asp-for="FinancialYear" class="form-control" />
                                @*<span asp-validation-for="FinancialYear" class="text-danger"></span>*@
                            </div>
                            <div class="form-group">
                                <input type="submit" value="Update" class="btn btn-outline-primary" />
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

